#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../schema-utils/node_modules/uglifyjs-webpack-plugin/node_modules/acorn-dynamic-import/node_modules/acorn/bin/acorn" "$@"
  ret=$?
else 
  node  "$basedir/../../../schema-utils/node_modules/uglifyjs-webpack-plugin/node_modules/acorn-dynamic-import/node_modules/acorn/bin/acorn" "$@"
  ret=$?
fi
exit $ret
